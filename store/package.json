{
    "name": "store",
    "version": "1.0.0",
    "description": "eCommerce Application",
    "main": "webpack.base.config.js",
    "scripts": {
        "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
        "build": "webpack --config ./webpack.config.js --env mode=prod",
        "lint": "eslint . --ext .ts",
        "fix": "eslint . --fix --ext .ts",
        "format": "npx prettier . --write",
        "ci:format": "npx prettier . --check",
        "deploy": "npm run build && npx gh-pages -d dist -e store",
        "test": "jest --coverage",
        "prepare": "husky init"
    },
    "author": "WebPunk",
    "license": "ISC",
    "devDependencies": {
        "@testing-library/dom": "^10.1.0",
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.12",
        "@types/node-fetch": "^2.6.11",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.10.0",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-webpack-plugin": "^4.1.0",
        "html-loader": "^5.0.0",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "msw": "^2.3.0",
        "prettier": "3.2.5",
        "style-loader": "^3.3.4",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.90.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-merge": "^5.10.0"
    },
    "dependencies": {
        "@commercetools/platform-sdk": "^7.8.0",
        "@commercetools/sdk-client-v2": "^2.5.0"
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
